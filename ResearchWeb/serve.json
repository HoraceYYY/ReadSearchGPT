{
    "headers": [
      {
        "source": "/index.html",
        "headers": [{
          "key": "Cache-Control",
          "value": "no-cache"
        }]
      },
      {
        "source": "**/*",
        "headers": [{
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }]
      }
    ]
  }
  